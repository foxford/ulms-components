{
  "plugins": [],
  "env": {
    "es": {
      "presets": [
        "@babel/preset-react",
        ["@babel/preset-env", {
          "modules": false,
          "targets":{
            "chrome": 43,
            "firefox": 43,
            "safari": 10,
            "ios": 10,
            "opera": 40,
            "edge": 13,
            "ie": 9
          }
        }]
      ],
      "plugins": [
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-proposal-object-rest-spread",
        "babel-plugin-annotate-pure-calls"
      ]
    }
  }
}
