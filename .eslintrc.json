{
  "extends": ["adequate-react"],
  "env": {
    "browser": true,
    "jquery": true
  },
  "globals": {
    "globalThis": true
  },
  "rules": {
    "node/no-unsupported-features/es-syntax": 0,
    "no-underscore-dangle": 0,
    "react/prop-types": 0,
    "max-len": ["error", { "code": 120, "tabWidth": 4, "ignoreTrailingComments": true, "ignoreStrings": true }]
  }
}
