{
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "_lint": "eslint --ext js,jsx",
    "preanalyze": "npm run build/dev",
    "analyze": "source-map-explorer ./packages/*/es/index.js",
    "build": "NODE_ENV=production lerna run build",
    "build/dev": "lerna run build",
    "precoverage": "rm -rf coverage",
    "coverage": "BABEL_ENV=jest jest ./packages --coverage --ci",
    "coverage/report": "npm run coverage && codecov --disable=gcov",
    "_prepare": "lerna list | sed 's/@ulms\\///g' | sed 's/ui-//g' | xargs -I{} scripts/populate-rollup.sh {}",
    "prepublishOnly": "npm run build",
    "test": "npm run test/lint && npm run test/all",
    "test/all": "BABEL_ENV=jest jest",
    "test/lint": "npm run _lint -- packages/*/src"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.16",
    "@babel/core": "^7.12.16",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-private-methods": "^7.24.1",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.12.13",
    "@babel/register": "^7.0.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-strip": "^3.0.2",
    "@rollup/plugin-terser": "^0.4.0",
    "@svgr/rollup": "~6.5.1",
    "@svgr/webpack": "~6.5.1",
    "autoprefixer": "^9.1.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.5",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "babel-plugin-react-css-modules": "^5.2.6",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "codecov": "^3.8.1",
    "concurrently": "^7.6.0",
    "core-js": "^3.8.3",
    "cssnano": "^4.1.11",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.7",
    "eslint": "^7.32.0",
    "eslint-config-adequate": "^2.0.2",
    "eslint-config-adequate-node": "^1.0.1",
    "eslint-config-adequate-react": "^2.0.2",
    "eslint-config-airbnb": "^18.0.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-unicorn": "^6.0.1",
    "eslint-restricted-globals": "^0.2.0",
    "jest": "^26.6.3",
    "lerna": "8.1.2",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "postcss": "^8.4.21",
    "postcss-cssnext": "^3.1.0",
    "postcss-discard-duplicates": "^4.0.2",
    "postcss-load-config": "^4.0.1",
    "postcss-loader": "^4.3.0",
    "postcss-preset-env": "^5.3.0",
    "postcss-url": "^8.0.0",
    "reserved-words": "^0.1.2",
    "rollup": "^2.79.1",
    "rollup-plugin-postcss": "^4.0.2",
    "source-map-explorer": "^2.5.2",
    "webpack": "^4.26.1"
  },
  "engines": {
    "node": ">=16.15",
    "npm": ">=8.3"
  }
}
