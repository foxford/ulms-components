{
  "name": "@netology-group/media-ui",
  "version": "0.0.10",
  "description": "UI components library",
  "repository": {
    "type": "git",
    "url": "https://github.com/netology-group/ulms-media-ui"
  },
  "license": "UNLICENSED",
  "author": "",
  "files": [
    "es",
    "lib",
    "packages"
  ],
  "main": "lib/components/index.js",
  "module": "es/components/index.js",
  "directories": {
    "lib": "src"
  },
  "scripts": {
    "babel": "babel src --copy-files",
    "prebuild": "rm -rf es packages lib && mkdir -p packages",
    "build": "npm run build/cjs && npm run build/es && npm run build/packages",
    "build/cjs": "cross-env BABEL_ENV=cjs npm run babel -- -d lib",
    "build/es": "cross-env BABEL_ENV=es npm run babel -- -d es",
    "build/packages": "cross-env BABEL_ENV=packages node rollup.js",
    "precoverage": "rm -rf .nyc_output",
    "coverage": "npm run tap -- --cov",
    "postcoverage": "tap --coverage-report=text-lcov && codecov",
    "docs": "npm run docs/build && http-server ./styleguide -p ${DOCS_PORT:-9090}",
    "docs/build": "cross-env BABEL_ENV=styleguidist npx styleguidist build",
    "docs/serve": "cross-env BABEL_ENV=styleguidist npx styleguidist server",
    "lint": "eslint --ext js,jsx src",
    "prepublishOnly": "npm run test && npm run build",
    "tap": "tap test/*.js",
    "test": "npm run lint && npm run tap"
  },
  "dependencies": {
    "@foxford/ui": "^1.2.1",
    "classnames": "^2.2.6",
    "debug": "^4.1.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-sizeme": "~2.5.2",
    "react-visibility-sensor": "~5.0.2",
    "recompose": "^0.30.0",
    "scroll-into-view-if-needed": "~2.2.20"
  },
  "devDependencies": {
    "@svgr/rollup": "~4.1.0",
    "@svgr/webpack": "~4.1.0",
    "autoprefixer": "^9.1.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-react-css-modules": "^3.4.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "codecov": "^3.1.0",
    "concurrently": "^3.6.1",
    "cross-env": "^5.2.0",
    "cssnano": "^4.1.7",
    "eslint-config-adequate-react": "^1.1.1",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^5.3.0",
    "postcss-url": "^8.0.0",
    "react-styleguidist": "^8.0.6",
    "rollup": "^0.67.3",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.5",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-sass": "^0.9.2",
    "rollup-plugin-scss": "^0.4.0",
    "rollup-plugin-uglify": "^3.0.0",
    "tap": "^12.1.1",
    "webpack": "^4.26.1"
  },
  "peerDependencies": {
    "js-cookie": "^2.2.0",
    "moment": "^2.22.2",
    "ramda": "^0.25.0",
    "react": "^16.4.2",
    "react-datepicker": "^1.6.0",
    "react-dom": "^16.4.2",
    "react-input-mask": "^2.0.3",
    "react-router-dom": "^4.3.1"
  },
  "engines": {
    "node": ">=8.10"
  }
}
