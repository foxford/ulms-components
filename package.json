{
  "name": "@ulms/components",
  "version": "1.2.0",
  "description": "UI components library",
  "homepage": "https://github.com/netology-group/ulms-media-ui",
  "bugs": {
    "url": "https://github.com/netology-group/ulms-media-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/netology-group/ulms-media-ui.git"
  },
  "license": "BSD-4-Clause",
  "author": "OLC Netology group LLC",
  "directories": {
    "lib": "packages"
  },
  "files": [
    "packages-v0",
    "lerna.json"
  ],
  "scripts": {
    "_lint": "eslint --ext js,jsx",
    "preanalyze": "npm run build/dev",
    "analyze": "source-map-explorer ./packages/*/es/index.js",
    "prebootstrap": "rm -rf ./packages/**/.*.sw*",
    "bootstrap": "lerna list | sed 's/@ulms\\/ui-//g' | xargs -I{} scripts/populate-rollup.sh {} && lerna bootstrap",
    "prebuild": "npm run prebootstrap",
    "build": "lerna exec 'NODE_ENV=production npm run build'",
    "build-storybook": "build-storybook",
    "build/dev": "lerna exec 'npm run build'",
    "precoverage": "rm -rf coverage",
    "coverage": "BABEL_ENV=jest jest ./packages --coverage --ci",
    "coverage/report": "npm run coverage && codecov --disable=gcov",
    "preold_build": "rm -rf packages-v0 && mkdir -p packages-v0",
    "old_build": "npm run old_build/packages",
    "old_build/package": "BABEL_ENV=packages node rollup.js",
    "old_build/packages": "./scripts/build-packages.sh",
    "prepare": "npm run bootstrap",
    "prepublishOnly": "npm run build && NODE_ENV=production npm run old_build",
    "start": "npm run storybook",
    "storybook": "start-storybook -p 6006",
    "test": "npm run test/lint && npm run test/all",
    "test/all": "BABEL_ENV=jest jest",
    "test/lint": "npm run _lint -- packages/*/src"
  },
  "dependencies": {
    "@foxford/ui": "^1.8.1",
    "@ungap/event-target": "^0.1.0",
    "classnames-es": "^2.2.6",
    "d3-queue": "~3.0.7",
    "debug": "^4.1.0",
    "fabric": "netology-group/fabric.js#feature/ULMS-509",
    "hammerjs": "~2.0.8",
    "lodash": "^4.17.20",
    "popper.js": "^1.15.0",
    "query-string": "^6.2.0",
    "rc-slider": "^8.6.9",
    "react": "^16.6.3",
    "react-color": "^2.17.3",
    "react-dom": "^16.6.3",
    "react-sizeme": "~2.6.7",
    "react-visibility-sensor": "~5.0.2",
    "recompose": "^0.30.0",
    "scroll-into-view-if-needed": "~2.2.20"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.16",
    "@babel/core": "^7.12.16",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.12.13",
    "@babel/register": "^7.0.0",
    "@storybook/addon-actions": "^6.1.17",
    "@storybook/addon-essentials": "^6.1.17",
    "@storybook/addon-links": "^6.1.17",
    "@storybook/addon-storysource": "^6.1.17",
    "@storybook/react": "^6.1.17",
    "@svgr/rollup": "~4.1.0",
    "@svgr/webpack": "~4.1.0",
    "autoprefixer": "^9.1.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.5",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "babel-plugin-react-css-modules": "^3.4.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "codecov": "^3.8.1",
    "concurrently": "^4.1.1",
    "core-js": "^3.8.3",
    "cssnano": "^4.1.7",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^6.8.0",
    "eslint-config-adequate": "^2.0.2",
    "eslint-config-adequate-node": "^1.0.1",
    "eslint-config-adequate-react": "^2.0.2",
    "eslint-config-airbnb": "^18.0.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-unicorn": "^6.0.1",
    "eslint-restricted-globals": "^0.2.0",
    "jest": "^26.6.3",
    "lerna": "^3.22.1",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-discard-duplicates": "^4.0.2",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^5.3.0",
    "postcss-url": "^8.0.0",
    "reserved-words": "^0.1.2",
    "rollup": "^0.67.3",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-babel": "^4.3.0",
    "rollup-plugin-commonjs": "^9.1.5",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-sass": "^0.9.2",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-uglify": "^3.0.0",
    "source-map-explorer": "^2.5.2",
    "storybook": "^6.1.17",
    "storybook-css-modules-preset": "^1.0.6",
    "tap": "^12.1.1",
    "webpack": "^4.26.1"
  },
  "peerDependencies": {
    "@foxford/ui": "^1.8.1",
    "fabric": "netology-group/fabric.js#feature/ULMS-509",
    "rc-slider": "~8.6.9",
    "react": "^15.6 || ^16.x",
    "react-color": "~2.17.0",
    "react-dom": "^15.6 || ^16.x",
    "react-sizeme": "~2.6.7",
    "react-visibility-sensor": "~5.0",
    "scroll-into-view-if-needed": "~2.2"
  },
  "optionalDependencies": {
    "react-router-dom": "^4.3.1"
  },
  "engines": {
    "node": ">=10.14"
  }
}
