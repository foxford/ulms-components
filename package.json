{
  "name": "@netology-group/media-ui",
  "version": "0.0.17",
  "description": "UI components library",
  "homepage": "https://github.com/netology-group/ulms-media-ui",
  "bugs": {
    "url": "https://github.com/netology-group/ulms-media-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/netology-group/ulms-media-ui.git"
  },
  "license": "BSD-4-Clause",
  "author": "OLC Netology group LLC",
  "files": [
    "es",
    "lib",
    "packages"
  ],
  "main": "lib/components/index.js",
  "module": "es/components/index.js",
  "directories": {
    "lib": "src"
  },
  "scripts": {
    "babel": "babel src --config-file ./.babelrc.json --copy-files",
    "prebuild": "rm -rf es packages lib && mkdir -p packages",
    "build": "npm run build/cjs && npm run build/es && npm run build/packages",
    "build/cjs": "cross-env BABEL_ENV=cjs npm run babel -- -d lib",
    "build/es": "cross-env BABEL_ENV=es npm run babel -- -d es",
    "build/packages": "cross-env BABEL_ENV=packages node rollup.js",
    "precoverage": "rm -rf .nyc_output coverage && mkdir -p coverage",
    "coverage": "npm run nodetap -- --nyc-arg=--require=./test/babel-register.js --cov",
    "postcoverage": "tap --coverage-report=text-lcov > coverage/coverage.lcov && codecov",
    "docs": "npm run docs/build && http-server ./styleguide",
    "docs/build": "cross-env BABEL_ENV=styleguidist styleguidist build",
    "docs/serve": "cross-env BABEL_ENV=styleguidist styleguidist server",
    "lint": "eslint --ext js,jsx src",
    "nodetap": "tap test/**/*.test.js",
    "prepublishOnly": "cross-env NODE_ENV=production npm run build",
    "start": "npm run docs/serve",
    "tap": "npm run nodetap -- --node-arg=--require=./test/babel-register.js",
    "test": "npm run lint && npm run tap"
  },
  "dependencies": {
    "@foxford/ui": "^1.3.6",
    "classnames": "^2.2.6",
    "debug": "^4.1.0",
    "lodash": "^4.17.11",
    "query-string": "^6.2.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-sizeme": "~2.5.2",
    "react-visibility-sensor": "~5.0.2",
    "recompose": "^0.30.0",
    "scroll-into-view-if-needed": "~2.2.20"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@svgr/rollup": "~4.1.0",
    "@svgr/webpack": "~4.1.0",
    "autoprefixer": "^9.1.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "babel-plugin-react-css-modules": "^3.4.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "codecov": "^3.1.0",
    "cross-env": "^5.2.0",
    "cssnano": "^4.1.7",
    "eslint-config-adequate-react": "^1.1.3",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-discard-duplicates": "^4.0.2",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^5.3.0",
    "postcss-url": "^8.0.0",
    "react-styleguidist": "^8.0.6",
    "reserved-words": "^0.1.2",
    "rollup": "^0.67.3",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-babel": "^4.3.0",
    "rollup-plugin-commonjs": "^9.1.5",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-sass": "^0.9.2",
    "rollup-plugin-scss": "^0.4.0",
    "rollup-plugin-uglify": "^3.0.0",
    "tap": "^12.1.1",
    "webpack": "^4.26.1"
  },
  "peerDependencies": {
    "react": "^15.6 || ^16.x",
    "react-dom": "^15.6 || ^16.x",
    "react-sizeme": "~2.5",
    "react-visibility-sensor": "~5.0",
    "scroll-into-view-if-needed": "~2.2"
  },
  "engines": {
    "node": ">=10.14"
  },
  "optionalDependencies": {
    "react-router-dom": "^4.3.1"
  }
}
