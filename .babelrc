{
  "plugins": [
    "babel-plugin-transform-object-rest-spread",
  ],
  "env":{
    "es": {
      "presets": [
        "babel-preset-react",
        ["babel-preset-env", {
          "modules": false,
          "targets": {
            "browsers": ["last 2 versions"]
          }
        }]
      ],
      "plugins": [
        "babel-plugin-annotate-pure-calls",
        "babel-plugin-transform-class-properties",
      ]
    },
    "cjs": {
      "presets": [
        "babel-preset-react",
        ["babel-preset-env", {
          "targets":{
            "chrome": 43,
            "firefox": 43,
            "safari": 10,
            "ios": 10,
            "opera": 40,
            "edge": 13,
            "ie": 9
          }
        }]
      ],
      "plugins": [
        "babel-plugin-annotate-pure-calls",
        "babel-plugin-transform-class-properties",
        "babel-plugin-transform-es2015-modules-commonjs"
      ]
    },
    "packages":{
      "presets": [
        "babel-preset-react",
        ["babel-preset-env", {
          "modules": false,
          "targets":{
            "chrome": 43,
            "firefox": 43,
            "safari": 10,
            "ios": 10,
            "opera": 40,
            "edge": 13,
            "ie": 9
          }
        }]
      ],
      "plugins": [
        "babel-plugin-external-helpers",
        "babel-plugin-annotate-pure-calls",
        "babel-plugin-transform-class-properties"
      ]
    },
    "styleguidist": {
      "presets": [
        "babel-preset-react",
        ["babel-preset-env", {
          "modules": false,
          "targets": {
            "browsers": ["last 2 versions"]
          }
        }]
      ],
      "plugins": [
        "babel-plugin-annotate-pure-calls",
        "babel-plugin-transform-class-properties",
        "babel-plugin-syntax-dynamic-import"
      ]
    },
  }
}
